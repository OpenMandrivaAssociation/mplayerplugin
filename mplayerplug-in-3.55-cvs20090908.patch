-	    shift_good_suffix[i] = j;
-	    ++needle_ptr;
-	}
-
-	shift_good_suffix[needle_len] = 0;
-	needle_ptr = (const unsigned char *) needle + needle_len - 1;
-	for (i = 1, j = 0; i < needle_len; ++i) {
